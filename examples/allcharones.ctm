/* MT que acepta sÃ³lo '1'*'0'?
   Deja el input intacto.
   Equivalente a allones.ctm, pero usando chars */

setup {
    alphabet {
       '1', '2';
    }
    blank_symbol = '0';
    max_tape_size = 20;
    max_number_transitions = 200;
}

alias {
}

declare {
    init state qGood;
    state qBad;
    final qGood;
}

transition {
    // lectura
    when (state qGood) {
        if (read '1') {
            header > 1;
        } else {
            goto qBad;
        }
    }
}
